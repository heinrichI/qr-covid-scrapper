SELECT vax_num, vax_id, fio, birthdate, passport, image_url, expiredat
FROM public.qr_covid
ORDER BY to_date(public.qr_covid.birthdate, 'DD.MM.YYYY');

SELECT *
FROM [Names].[dbo].[russian_surnames]
WHERE Surname LIKE N'Бор%' AND LEN(Surname) = 6
ORDER By PeoplesCount

SELECT Name + N'ович', Sex, PeoplesCount
FROM [Names].[dbo].russian_names
WHERE Name LIKE N'А%' AND LEN(Name) = 13 - 4
AND Sex = N'М'
ORDER By PeoplesCount Desc